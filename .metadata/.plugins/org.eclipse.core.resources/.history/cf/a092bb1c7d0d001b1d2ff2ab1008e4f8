
public class Ejemplo7 {

	public static void main(String[] args) {

		// Diferencia entre print y println.
		System.out.println("Hola, me llamo Carlos");
		System.out.println("Esto es la clase de Programaci칩n");

		System.out.println();

		System.out.print("Hola, me llamo Carlos");
		System.out.println("Esto es la clase de Programaci칩n");

		// Concatenaci칩n de cadenas (Strings).
		int edad = 30;
		System.out.println("Tienes " + edad + " a침os.");

		float precio = 90.50f;
		System.out.println("El precio son " + precio + " euros.");

	}

}
